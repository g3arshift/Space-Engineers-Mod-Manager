<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="inputInstructions" text="Enter a name for the profile:" />
            <TextField fx:id="input" minWidth="450.0" promptText="Profile name" />
            <HBox>
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <ButtonBar buttonOrder="U+FBIX_NCYOA_R">
                    <buttons>
                      <Button fx:id="accept" mnemonicParsing="false" text="OK" />
                        <Button fx:id="cancel" cancelButton="true" mnemonicParsing="false" text="Cancel" />
                    </buttons>
                  </ButtonBar>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
