<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" minWidth="200.0" spacing="10.0">
               <children>
                  <Label fx:id="saveName">
                     <graphic>
                        <Label text="Selected Save:" />
                     </graphic>
                  </Label>
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="chooseSave" mnemonicParsing="false" onAction="#chooseSave" text="Select Save" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <ButtonBar buttonOrder="U+FBIX_NCYOA_R">
                    <buttons>
                      <Button fx:id="addSave" mnemonicParsing="false" onAction="#addSave" text="Next" />
                        <Button fx:id="cancelAddSave" cancelButton="true" mnemonicParsing="false" onAction="#cancelAddSave" text="Cancel" />
                    </buttons>
                  </ButtonBar>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
