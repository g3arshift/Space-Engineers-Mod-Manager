<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minWidth="350.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" minWidth="200.0" spacing="10.0">
               <children>
                  <Label fx:id="saveName">
                     <graphic>
                        <Label text="Save:" />
                     </graphic>
                  </Label>
                  <Region HBox.hgrow="ALWAYS" />
                  <Button fx:id="chooseSave" minWidth="95.0" mnemonicParsing="false" onAction="#chooseSave" prefWidth="76.0" text="Select Save" />
               </children>
            </HBox>
            <HBox>
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <HBox spacing="5.0">
                     <children>
                      <Button fx:id="addSave" minWidth="75.0" mnemonicParsing="false" text="Next" />
                        <Button fx:id="cancelAddSave" cancelButton="true" minWidth="75.0" mnemonicParsing="false" text="Cancel" />
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
