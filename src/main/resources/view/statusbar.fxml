<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>


 <HBox alignment="CENTER_RIGHT" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
     <children>
         <Label fx:id="lastInjected">
             <graphic>
                 <Label text="Mods Last Applied:" />
             </graphic>
         </Label>
         <Region HBox.hgrow="ALWAYS" />
         <Label fx:id="lastModifiedBy">
             <graphic>
                 <Label text="Save Last Modified by:" />
             </graphic>
         </Label>
         <Label fx:id="saveStatus">
             <graphic>
                 <Label text="Status:" />
             </graphic>
         </Label>
     </children>
     <padding>
         <Insets bottom="5.0" left="5.0" right="5.0" />
     </padding>
     <opaqueInsets>
         <Insets />
     </opaqueInsets>
 </HBox>